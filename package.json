{
  "name": "matching-game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/compat-data": "^7.22.20",
    "@babel/core": "^7.22.20",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-enzyme": "^7.1.2",
    "jest-expect-message": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "style-loader": "^3.3.3",
    "web-vitals": "^3.4.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode development --env appFoobar --env titleQwerty",
    "start-refactor": "webpack-dev-server --open --mode development --env.entryDir=refactor",
    "build": "webpack --mode development --config webpack.config.js",
    "test": "jest",
    "test:module-2": "jest test/module-2/",
    "test:module-3": "jest test/module-3/",
    "test:module-4": "jest test/module-4/",
    "test:module-5": "jest test/module-5/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js",
      "jest-expect-message"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15"
  }
}
